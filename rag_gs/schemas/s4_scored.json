{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["qid", "doc_id", "grade_1_5", "model"],
  "properties": {
    "qid": {"type": "string"},
    "doc_id": {"type": "string"},
    "grade_1_5": {"type": "integer", "minimum": 1, "maximum": 5},
    "model": {"type": "string"},
    "temperature": {"type": "number"},
    "top_p": {"type": "number"},
    "ts": {"type": "string"},
    "source": {"type": ["string", "null"]},
    "rank_dense": {"type": ["integer", "null"]},
    "rank_sparse": {"type": ["integer", "null"]},
    "score_cos": {"type": ["number", "null"]},
    "score_es": {"type": ["number", "null"]},
    "score_bm25": {"type": ["number", "null"]},
    "rrf_score": {"type": ["number", "null"]},
    "rank_rrf": {"type": ["integer", "null"]}
  }
}

